{
    "name": "hasheado/admin-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 bundle based on SonataAdminBundle to generate admins",
    "keywords": ["synfony", "bundle", "admin", "admin generator"],
    "homepage": "http://github.com/Hasheado/AdminBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Emiliano Viada",
            "email": "emjovi@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": ">=2.3",
        "sonata-project/admin-bundle": "~2.3",
        "sonata-project/doctrine-orm-admin-bundle": "~2.3",
        "sonata-project/easy-extends-bundle": "~2.1.9",
        "sonata-project/user-bundle": "2.2.x"
    },
    "require-dev": {
        
    },
    "autoload": {
        "psr-4": { "Hasheado\\AdminBundle": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "Hasheado/AdminBundle"
}
